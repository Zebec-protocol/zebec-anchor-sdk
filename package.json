{
	"name": "zebec-protocol",
	"version": "1.0.0",
	"description": "Typescript SDKs for the Zebec Protocol",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Zebec-protocol/zebec-sdk-v2.git"
	},
	"keywords": [
		"zebec protocol",
		"money streaming",
		"token streaming",
		"zebec"
	],
	"workspaces": [
		"packages/*"
	],
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Zebec-protocol/zebec-sdk-v2/issues"
	},
	"homepage": "https://github.com/Zebec-protocol/zebec-sdk-v2/#readme",
	"files": [
		"packages/**/dist"
	],
	"scripts": {
		"build": "tsc -b ./packages/**/tsconfig.json",
		"clean": "rimraf ./packages/**/dist",
		"clean:win": "rmdir /s packages/**/dist",
		"install:stream": "cd ../../../ && npm i -S @zebec-io/stream",
		"docs": "typedoc"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": "Ashish | Zebec Protocol"
}